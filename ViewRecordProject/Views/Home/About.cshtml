
@model List<ViewRecordProject.Models.Faculty>;

<br />
<br />

<div class="container">

    <table class="table table-bordered">
        <thead>
            <tr>
                
                <th>Id</th>
                <th>Name</th>
                <th>Depertment</th>
            </tr>
        </thead>
        <tbody>
            @*@i = 0;*@
            @foreach (var x in Model)

            {


                <tr>
                    @*<td>@i</td>*@
                    <td>@x.id</td>
                    <td>@x.name</td>
                    <td>@x.depertment</td>

                </tr>

            }

        </tbody>
    </table>
</div>
